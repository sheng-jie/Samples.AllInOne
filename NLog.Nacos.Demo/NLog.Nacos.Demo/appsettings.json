{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "NacosConfig": {
    "Listeners": [
      {
        "Optional": false,
        "DataId": "nlog.json",
        "Group": "global"
      },
      {
        "Optional": false,
        "DataId": "nlog-demo.appsettings.json",
        "Group": "nlog-demo"
      }
    ]
  }
}
